{
  "name": "form-elements",
  "version": "2.2.3",
  "description": "Form elements for HTML-element based systems",
  "main": "form-elements.js",
  "scripts": {
    "test": "standard && npm run dep && node test/ssr-rendering.js && browserify test/*.spec.js | tape-run",
    "coverage": "npm run test && node test-harness.js",
    "start": "bankai start standalone.js --debug --no-uglify",
    "dep": "dependency-check . && dependency-check . --unused --no-dev -i envify -i yo-yoify -i babelify -i babel-preset-env -i sheetify"
  },
  "browserify": {
    "transform": [
      "envify",
      "yo-yoify",
      "sheetify/transform",
      "babelify"
    ]
  },
  "keywords": [
    "bel",
    "choo",
    "forms",
    "form",
    "validation"
  ],
  "author": "Todd Kennedy <todd@selfassembled.org>",
  "license": "Apache-2.0",
  "dependencies": {
    "babel-preset-env": "^1.5.2",
    "babelify": "^7.3.0",
    "bel": "^4.6.0",
    "envify": "^4.0.0",
    "sheetify": "^6.1.0",
    "yo-yoify": "^3.7.3"
  },
  "devDependencies": {
    "bankai": "^7.6.2",
    "browserify": "^14.3.0",
    "browserify-istanbul": "github:toddself/browserify-istanbul#strip-base-path",
    "dependency-check": "^2.9.1",
    "istanbul": "^0.4.5",
    "nanobus": "^4.0.0",
    "opener": "^1.4.3",
    "run-series": "^1.1.4",
    "standard": "^10.0.2",
    "tape": "^4.6.3",
    "tape-istanbul": "^1.1.1",
    "tape-run": "^3.0.0"
  }
}
